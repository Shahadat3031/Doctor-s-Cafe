<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.a6te.lazycoder.doctorscafe">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="net.a6te.lazycoder.doctorscafe.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddDoctor"
            android:label="Doctor's Factory"/>
        <activity android:name=".DoctorsList"
            android:label="Doctor's List"/>
        <activity android:name=".DoctorDetails"
            android:label="Doctor's Details"/>
        <activity android:name=".PrescriptionActivity" />
        <activity android:name=".SignUp"
            android:label="Sign Up">

        </activity>
    </application>

</manifest>